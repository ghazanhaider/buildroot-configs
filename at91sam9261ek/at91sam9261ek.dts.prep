      clock-frequency = <4965000>;
      hactive = <240>;
      vactive = <320>;
      hback-porch = <1>;
      hfront-porch = <33>;
      vback-porch = <1>;
      vfront-porch = <0>;
      hsync-len = <5>;
      vsync-len = <1>;
      hsync-active = <1>;
      vsync-active = <1>;
     };
    };
   };
  };

  ebi: ebi@10000000 {
   status = "okay";

