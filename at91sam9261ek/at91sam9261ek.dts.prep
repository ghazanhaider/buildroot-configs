 compatible = "atmel,at91sam9261ek", "atmel,at91sam9261", "atmel,at91sam9";

 chosen {
  bootargs = "rootfstype=ubifs ubi.mtd=5 root=ubi0:rootfs rw";
  stdout-path = "serial0:115200n8";
